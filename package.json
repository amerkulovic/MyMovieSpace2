  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "framer-motion": "^11.15.0",
    "mongodb": "4.1",
    "mongoose": "^7.4.3",
    "react-icons": "^4.8.0"
  }
}
